#:import utils kivy.utils


<BoubleButton@Button>:
    background_color: 0,0,0,0
    background_normal: ''
    #color: .251, .878, .816, 1
    pos_hint: {'center_x': 0.5}
    size_hint: (None,None)
    width: 90
    height: 60
    canvas.before:
        Color: 
            rgba: (177/255.0,179/255.0,181/255.0,1) 
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [100]

<Label@Label>:
    pos_hint: {'center_x': 0.5}
    size_hint: (None,None)
    width: 40
    height: 60

